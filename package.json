{
  "name": "xity-starter",
  "version": "2.0.0",
  "private": true,
  "description": "A blog-ready 11ty starter based on PostCSS and Parcel, with a RSS feed and Native Elements.",
  "author": "Mattia Astorino",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/equinusocio/xity-starter"
  },
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test": "echo \"This template does not include a test runner by default.\" && exit 1",
    "lint": "prettier --write .",
    "update:deps": "yarn upgrade-interactive --latest && yarn upgrade",
    "update:cssdb": "npx browserslist@latest --update-db"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "@snowpack/plugin-postcss": "^1.4.3",
    "@snowpack/plugin-run-script": "^2.3.0",
    "cssnano": "^5.0.8",
    "cssnano-preset-advanced": "^5.1.4",
    "markdown-it-attrs": "^4.1.0",
    "postcss": "^8.3.9",
    "postcss-cli": "^8.3.1",
    "postcss-dark-theme-class": "^0.5.1",
    "postcss-import": "^14.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "snowpack": "^3.8.8"
  },
  "dependencies": {
    "@kevingimbel/eleventy-plugin-codepen": "^1.0.1",
    "@tbranyen/jsdom": "^13.0.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-functional": "^3.7.2",
    "eslint-plugin-import": "^2.25.2",
    "is-svg": "^4.2.2",
    "slugify": "^1.6.1",
    "xmlhttprequest-ssl": "^1.6.2"
  }
}
